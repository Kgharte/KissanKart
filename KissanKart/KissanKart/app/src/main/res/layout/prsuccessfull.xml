<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/texture"

    tools:context="com.example.meet.kissankart.PRSuccessfull">

    <ImageView
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:id="@+id/imageView4"
        android:src="@mipmap/donesign"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Congratulations !!!"
        android:textColor="#ffffff"
        android:textSize="30sp"
        android:id="@+id/textView9"
        android:layout_below="@+id/imageView4"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="New Password Generated Successfully"
        android:id="@+id/textView10"
        android:paddingLeft="15dp"
        android:textColor="#ffffff"
        android:layout_marginTop="15dp"
        android:textSize="20dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Please Login with your new Password"
        android:paddingLeft="15dp"
        android:id="@+id/textView11"
        android:textColor="#ffffff"
        android:layout_marginTop="87dp"
        android:textSize="20dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/loginbuttonpart"
        android:layout_marginTop="15dp"
        android:id="@+id/btnnewlogin"
        android:layout_below="@+id/textView11"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
